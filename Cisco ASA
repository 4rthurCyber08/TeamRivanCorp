!@CiscoASA-69
conf t
 hostname CiscoFW-72
 enable password pass
 int g1/1
  nameif FW-TO-EDGE
  security-level 100
  no shut
  ip add 10.72.72.12 255.255.255.0
  exit
 int g1/2
  nameif INSIDE
  security-level 100
  no shut
  ip add 10.72.69.12 255.255.255.0
  exit
 int man1/1
  management-only
  no nameif
  no security-level
  no ip address
  no shutdown
  exit
 !
 no router ospf 1
 router ospf 1
  router-id 10.72.72.12
  network 10.72.72.0 255.255.255.0 area 0
  network 10.72.69.0 255.255.255.0 area 0
  end
  


!@CiscoASA-72
conf t
 username admin password pass privi 15
 http server enable
 http 10.72.1.0 255.255.255.0 INSIDE
 end


!@CiscoASA-72
dir disk0:


!@CiscoASA-72
conf t
asdm image disk0:/asdm-782.bin
end




