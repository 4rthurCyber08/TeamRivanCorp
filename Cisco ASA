!@coreTaas
conf t
 hostname coreTaas-72
 enable secret pass
 service password-encryption
 no logging console
 no ip domain-lookup
 line cons 0
  password pass
  login
  exec-timeout 0 0
 line vty 0 14
  password pass
  login
  exec-timeout 0 0
 int vlan 1
  no shut
  ip add 10.72.1.2 255.255.255.0
  desc DEFAULT-VLAN
 int vlan 10
  no shut
  ip add 10.72.10.2 255.255.255.0
  desc WIFI-VLAN
 int vlan 50
  no shut
  ip add 10.72.50.2 255.255.255.0
  desc CCTV-VLAN
 int vlan 100
  no shut
  ip add 10.72.100.2 255.255.255.0
  desc VOICE-VLAN
 end

!@CoreTAAS
conf t
 vlan 69
  name FW
  exit
 int vlan 69
  ip add 10.72.69.2 255.255.255.0
  no shut
  exit
 int range fa0/1,fa0/3
  switchport mode access
  switchport access vlan 69
  exit
 !
 ip routing
 router ospf 1
  router-id 10.72.1.2
  network 10.72.1.0  0.0.0.255 area 0
  network 10.72.10.0 0.0.0.255 area 0
  network 10.72.69.0 0.0.0.255 area 0
  network 10.72.50.0 0.0.0.255 area 0
  network 10.72.100.0 0.0.0.255 area 0
  end




